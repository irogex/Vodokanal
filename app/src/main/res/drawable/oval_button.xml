<?xml version="1.0" encoding="utf-8"?>
<selector
    xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="false">
        <shape android:shape="oval">
            <gradient android:angle="90" android:startColor="#ffd823" android:endColor="#f5f7fa" android:type="linear"/>
            <stroke android:width="2dp" android:color="#44ccff"/>
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape android:shape="oval">
            <gradient android:angle="90" android:startColor="#c3cfe2" android:endColor="#f5f7fa" android:type="linear"/>
            <stroke android:width="2dp" android:color="#44ccff"/>
        </shape>
    </item>

</selector>
